<html>
  <head>
    <script type="text/javascript">
      google.charts.load("current", {packages:["corechart"]});
      google.charts.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Task', 'Hours per Day'],
          ['Work',     11],
          ['Eat',      2],
          ['Commute',  2],
          ['Watch TV', 2],
          ['Sleep',    7]
        ]);

        var options = {
          title: 'My Daily Activities',
          is3D: true,
          width:900,
          height: 500,
        };

        var chart = new google.visualization.PieChart(document.getElementById('inactivite'));
        chart.draw(data, options);
      }
    </script>
  </head>
  <body>
        <h2 align="center">Graphe Implication - Inactivit√© sur les tickets</h2>
    <div class="Gqualite" id="inactivite" style="width: 900px; height: 500px;"></div>
  </body>
</html>